    "keywords": ["tmdb", "api", "php","wrapper", "movie", "cinema", "tv", "tv show", "tvdb", "symfony", "symfony2", "symfony3"],
    "authors": [
        {
            "name": "David C",
            "homepage": "https://github.com/davdi1982",
            "email": "davdi1982@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "symfony/config": "^4.3.7 || ^5.0",
        "symfony/dependency-injection": "^4.3.7 || ^5.0",
        "symfony/event-dispatcher": "^4.3.7 || ^5.0",
        "symfony/http-kernel": "^4.3.7 || ^5.0",
        "doctrine/doctrine-cache-bundle": "^1.0",
        "php-tmdb/api": "^2.1",
        "twig/twig": "^1.11|^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8||^5.0,<5.4",
        "symfony/framework-bundle": "^4.3.7 || ^5.0"
